<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/proBar3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Silver"
    android:orientation="vertical"
    android:padding="@dimen/p10"
    tools:context=".M0803">

    <TextView
        android:id="@+id/m0803_t001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/Gray"
        android:text="@string/m0803_t001"
        android:textColor="@color/Yellow"
        android:textSize="@dimen/fontsize" />

    <ProgressBar
        android:id="@+id/m0803_p001"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/p20"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/Gray"
        android:max="100"
        android:progress="30"
        android:secondaryProgress="60" />

    <TextView
        android:id="@+id/m0803_t002"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/Teal"
        android:text="@string/m0803_t002"
        android:textColor="@color/Yellow"
        android:textSize="@dimen/fontsize" />

    <ProgressBar
        android:id="@+id/m0803_p002"
        style="@android:style/Widget.DeviceDefault.ProgressBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/Teal" />

    <TextView
        android:id="@+id/m0803_t003"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/colorPrimary"
        android:text="@string/m0803_t003"
        android:textColor="@color/Yellow"
        android:textSize="@dimen/fontsize" />

    <ProgressBar
        android:id="@+id/m0803_p003"
        style="@android:style/Widget.DeviceDefault.Light.ProgressBar.Large"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/colorPrimary" />

    <TextView
        android:id="@+id/m0803_t004"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/Olive"
        android:text="@string/m0803_t004"
        android:textColor="@color/Yellow"
        android:textSize="@dimen/fontsize" />

    <ProgressBar
        android:id="@+id/m0803_p004"
        style="?android:attr/progressBarStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p10"
        android:background="@color/Olive" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/p10"
        android:gravity="end"
        android:text="@string/myname"
        android:textColor="@color/Yellow"
        android:textSize="@dimen/fontsize" />

</LinearLayout>